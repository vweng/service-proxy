{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://secure.toronto.ca/cc_api/svcaccount_v1/ParkingTicket/view",
  "type": "object",
  "oneOf": [
{ "additionalProperties": false,
"required": [ "TICKET_NUMBER", "LAST_NAME"],
"properties": {
    "TICKET_NUMBER": {
      "type": "string",
      "pattern": "^[a-z_A-Z]{2}\\d{6}$|^\\d{8}$",
      "maxLength":8
    },
    "LAST_NAME": {
      "type": "string",
      "maxLength":3
    }
}},
{ "additionalProperties": false,
"required": [ "RIN", "STATUS"],
"properties": {
    "RIN": {
      "type": "string",
      "pattern": "^\\d{1,9}$", 
      "maxLength":9
    },
    "STATUS": {
      "type": "string",
      "pattern": "\\d{1,}[,\\d{1,}]{0,}", 
      "maxLength":5
    },
    "offset": {
      "type": "string",
      "pattern": "^\\d{1,}$",
      "maxLength":20
    },
    "limit": {
      "type": "string",
      "pattern": "^\\d{1,}$",
      "maxLength":20
    }
}},
{ "additionalProperties": false,
"required": [ "DRIVERLICENSE_NUMBER", "STATUS"],
"properties": {
    "DRIVERLICENSE_NUMBER": {
      "type": "string",
      "pattern": "^[a-z_A-Z]{1}\\d{4}-\\d{5}-\\d{5}$",
      "maxLength":17
    },
    "STATUS": {
      "type": "string",
      "pattern": "\\d{1,}[,\\d{1,}]{0,}",
      "maxLength":5
    },
    "offset": {
      "type": "string",
      "pattern": "^\\d{1,}$",
      "maxLength":20
    },
    "limit": {
      "type": "string",
      "pattern": "^\\d{1,}$",
      "maxLength":20
    }
}}
]

}